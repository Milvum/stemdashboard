{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-ordering": false,
    "variable-name": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-unused-variable": [
      true
    ],
    "no-unused-expression": [
      true,
      "allow-fast-null-checks"
    ],
    "no-bitwise": false
  }
}
